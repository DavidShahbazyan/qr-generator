<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>


<?import javafx.geometry.Insets?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.layout.HBox?>
<VBox spacing="25.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="am.davsoft.qrgen.controllers.subViews.VCardSubViewController">
    <children>
        <HBox spacing="10.0">
            <children>
                <JFXTextField focusColor="ORANGE" labelFloat="true" promptText="Name" HBox.hgrow="ALWAYS" />
                <JFXTextField focusColor="ORANGE" labelFloat="true" promptText="Company" HBox.hgrow="ALWAYS" />
            </children>
        </HBox>
        <HBox spacing="10.0">
            <children>
                <JFXTextField focusColor="ORANGE" labelFloat="true" promptText="Title" HBox.hgrow="ALWAYS" />
                <JFXTextField focusColor="ORANGE" labelFloat="true" promptText="Phone" HBox.hgrow="ALWAYS" />
            </children>
        </HBox>
        <HBox spacing="10.0">
            <children>
                <JFXTextField focusColor="ORANGE" labelFloat="true" promptText="Website" HBox.hgrow="ALWAYS" />
                <JFXTextField focusColor="ORANGE" labelFloat="true" promptText="Email" HBox.hgrow="ALWAYS" />
            </children>
        </HBox>
        <HBox spacing="10.0">
            <children>
                <JFXTextField focusColor="ORANGE" labelFloat="true" promptText="Address" HBox.hgrow="ALWAYS" />
                <JFXTextField focusColor="ORANGE" labelFloat="true" promptText="Note" HBox.hgrow="ALWAYS" />
            </children>
        </HBox>
    </children>
    <padding>
        <Insets top="15.0" />
    </padding>
</VBox>
